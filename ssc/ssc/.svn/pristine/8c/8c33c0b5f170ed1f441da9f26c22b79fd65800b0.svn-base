<body class="gray-bg">
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-sm-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>香港六合彩开奖时间添加</small></h5>
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
            
                        </div>
                    </div>
                    <div class="ibox-content">

                        <form method="post" class="form-horizontal" action="__URL__/liuheAdd">
                            <div class="hr-line-dashed"></div>
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group m-b"><span class="input-group-addon">采种名称</span>
                                        <input type="text" placeholder="香港六合彩" value="香港六合彩" class="form-control">
                                    </div>
                                </div>
                            </div>
                             <div class="hr-line-dashed"></div>
                            <div class="form-group" id="data_1">
                                <div class="col-sm-12">
                                    <div class="input-group date">
                                        <span class="input-group-addon">开奖日期</span>
                                        <input type="text" class="form-control" name="date" value="">
                                    </div>
                                </div>
                            </div>
                             <div class="hr-line-dashed"></div>
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <div class="input-group m-b"><span class="input-group-addon">开奖期数</span>
                                        <input type="text" placeholder="开奖期数" value="{$period}" name="period"  class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="hr-line-dashed"></div>
                            <div class="form-group ">
                                <div class="col-sm-12">
                                    <div class="input-group m-b date">
                                    <span class="input-group-addon">开奖时间</span>
                                        <input type="text" placeholder="开奖期数" value="21:40:00" name="open_time" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="hr-line-dashed"></div>
                            <div class="form-group">
                                <div class="col-sm-4 col-sm-offset-2">
                                    <button class="btn btn-primary" type="submit">保存内容</button>
                                    <button class="btn btn-white" onClick="javascript :history.go(-1);">取消</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="__PUBLIC__/js/jquery.min.js?v=2.1.4"></script>
