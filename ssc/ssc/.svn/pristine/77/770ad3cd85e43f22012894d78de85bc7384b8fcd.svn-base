		<script>
			$(function() {
				var i = 0;
				var clone = $(".banner .img li").first().clone();
				$(".banner .img").append(clone);
				var size = $(".banner .img li").size();
				for(var j = 0; j < size - 1; j++) {
					$(".banner .num").append("<li></li>");
				}
				$(".banner .num li").first().addClass("on");
				$(".banner .num li").hover(function() {
					var index = $(this).index();
					i = index;
					$(".banner .img").stop().animate({ left: -index * 1920 }, 500);
					$(this).addClass("on").siblings().removeClass("on")
				})
				var t = setInterval(function() {
					i++;
					move()
				}, 5000);
				$(".banner").hover(function() {
					clearInterval(t);
				}, function() {
					t = setInterval(function() {
						i++;
						move()
					}, 5000)
				});
				$(".banner .btn_l").click(function() {
					i--;
					move();
				});
				$(".banner .btn_r").click(function() {
					i++;
					move()
				});

				function move() {

					if(i == size) {
						$(".banner  .img").css({ left: 0 });
						i = 1;
					}
					if(i == -1) {
						$(".banner .img").css({ left: -(size - 1) * 1920 });
						i = size - 2;
					}
					$(".banner .img").stop().animate({ left: -i * 1920 }, 500);

					if(i == size - 1) {
						$(".banner .num li").eq(0).addClass("on").siblings().removeClass("on")
					} else {
						$(".banner .num li").eq(i).addClass("on").siblings().removeClass("on")
					}
				}
			});
			/*公告 */
			var notice = function() {
				var i;

				function movechar() {
					if(i <= "-980px") {
						$("#notice_bar").css({ left: 1100 })
					}
					$("#notice_bar").stop().animate({ left: -1100 }, 20000)
				}
				var tim = setInterval(function() {
					i = $("#notice_bar").css('left');
					movechar()
				}, 20000)
			}();
		</script>
		<script>
			//header_menu
			$(document).ready(function() {
			$(".header_menu li a").click(function() {
				switch($(this).data("id")) {
					case 'sy':
						$('#ym_home').load('home.html');
						break;
					case 'yhhd':
						$('#ym_home').load('activity.html');
						break;
					case 'xljc':
						$(".xljc_tanc").show();
						break;
					default:
						return false
				}
