			$(document).on("click",".xljc_tanc_top a",function(){
				clearInterval(jishi_home);
				$(".xljc_tanc").hide();
			});
			});
		</script>
		 <script>

        		var autourl=new Array();
				var tim_home = 1;
				var bb = 1;
				var jishi_home;


        function getping(){
            bb = 1;	tim_home = 1;
            clearInterval(jishi_home);
			jishi_home = setInterval('tim_home++;console.log(tim_home+"hh");if(tim_home>202){clearInterval(jishi_home);}', 100);
        	$.ajax({
                            type: "GET",
                            url: "{:U('Html/Getdata/getaurlping')}",

                            async: true,
                            timeout: 10000,
                            success: function(data) {
                                    $("#urllist").empty();
                                    var html="";
                                        for (var i = 0; i < data.length; i++) {
                                            a=i+1;
                                            html+='<dd><span id="span_color'+a+'">测速中</span><i id="time'+a+'"></i><a id="links'+a+'" href="javascript:void(0);">进入</a><div id="host'+a+'"></div></dd>';
                                            autourl[a]='http://'+data[i].url;

                                        }
                                        $("#urllist").append(html);
                                        run();
                                }

                            })
        }
        
        $(document).on("click",".header_menu ul li a",function(){
					if($(this).data("id")=="xljc"){
						getping();
					}
				});
        function auto(url){
            //console.log(url);
                document.getElementById("host"+bb).innerHTML = url;
                document.getElementById("links"+bb).href = url;
                console.log(bb);
                if(tim_home<=3){
                     document.getElementById("time"+bb).innerHTML = tim_home*10+"ms";
                     $("#span_color"+bb).attr('class','span_0');
                     $("#span_color"+bb).html('极速');
                }
                if(tim_home>3 && tim_home<12){
                     document.getElementById("time"+bb).innerHTML = tim_home*10+"ms";
                     $("#span_color"+bb).attr('class','span_1');
                     $("#span_color"+bb).html('正常');
                }
                if(tim_home>12 && tim_home<200){
                     document.getElementById("time"+bb).innerHTML = tim_home*10+"ms";
                     $("#span_color"+bb).attr('class','span_2');
                     $("#span_color"+bb).html('缓慢');
                }
                if( tim_home>200)
                {
                    document.getElementById("time"+bb).innerHTML = "超时";
                    $("#span_color"+bb).attr('class','span_3');
                    $("#span_color"+bb).html('超时');
                }

           
            bb++;
        }

        function run(){
        	 $(".cesu").remove();
            for(var i=1;i<autourl.length;i++){
                $("body").append("<img src="+autourl[i]+"/"+Math.random()+" class='cesu' width=1 height=1 onerror=auto('"+autourl[i]+"') style='display:none'>");
                }
            
        }
        
        
    </script>
		<!--弹出层-->
	
		<script type="text/javascript">
			// 用于判断是否发送请求完成
			var flag = true;
			if(flag) {
				flag = false;
				$('#section .fr').click(function() {
					//模态框显示
					$('#m_modal').fadeIn();
					// 阻止屏幕滚动
					//					$(window).scroll(function(e) {
					//						$(this).css({ "position": "relative" })
					//						$(this).scrollTop(0);
					//					});
					// 发送请求	
					$.get("{:U('Html/Notice/notice')}",function(resdata) {
							var innerHTML1 = null;
							var innerHTML2 = null;
							// 转化时间格式
							function addTime(time) {
								var time = new Date(time * 1000);
								return time.getFullYear() + '-' + (time.getMonth() + 1) + '-' + time.getDate() + ' ' + time.getHours() + ":" + time.getMinutes() + ":" + time.getSeconds();
							}
							for(var i = 0; i < resdata.length; i++) {
								innerHTML1 = '<li class="inform_item" data-index="' + i + '"><p>' + resdata[i].title + '</p><p>' + addTime(resdata[i].addtime) + '</p><span class="globule"></span></li>';
								innerHTML2 = '<div class="popups_body_right_container"><h2>' + resdata[i].title + '</h2><p>' + resdata[i].content + '</p></div>';
								$('#m_modal .popups_body_left .inform_list').append(innerHTML1);
								$('#m_modal .popups_body_right').append(innerHTML2);
							}
							$('#m_modal  .inform_item:first').addClass('inform_active');
							$('#m_modal  .inform_item').each(function(i, value) {
								// 首次判断选择的内容
								if($(this).hasClass('inform_active')) {
									$('#m_modal .popups_body_right_container').eq($(this).index()).css("display", "block");
								}
							});
							// 开启请求
							flag = true;
					});
				});
			}
			// 关闭模态框
			$('#m_modal .popups_close').click(function() {
				$('#m_modal').fadeOut();
				// 清空数据
				$('#m_modal .popups_body_left .inform_list li').remove();
				$('#m_modal .popups_body_right div').remove();
				//解绑滚动
				//				$(window).unbind('scroll');
			});
			// 点击对应的消息
			$('#m_modal .inform_list').on('click', '.inform_item', function() {
				$('.inform_item').removeClass('inform_active');
				$(this).addClass('inform_active');
				$('#m_modal .popups_body_right_container').css("display", "none");
				$('#m_modal .popups_body_right_container').eq($(this).data().index).css("display", "block");
			})
		</script>
