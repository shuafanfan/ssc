html,
body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
em,
form,
fieldset,
input,
textarea,
p,
span,
strong,
input,
i,
blockquote,
th,
td,
a {
	margin: 0;
	padding: 0;
	font-family: "Microsoft YaHei", Arial;
	font-style: normal
}

body {
	font-family: "Microsoft YaHei", Arial;
	font-size: 14px;
	color: #000000;
}

button,
input,
select,
textarea {
	font: 14px/1.5 Arial, Helvetica, Verdana, sans-serif;
	word-wrap: break-word;
	color: #565756;
}

ul,
li {
	list-style: none;
	margin: 0;
	padding: 0;
}

h1,
h2,
h3,
h4,
h5,
h6 {
	font-weight: normal;
	font-size: 100%;
}

img {
	border: none;
	border: 0;
	vertical-align: middle;
}

a {
	text-decoration: none;
	outline: none;
}

p {
	margin: 0;
	padding: 0;
}

input,
select,
textarea {
	vertical-align: middle;
	*font-size: 100%;
}

input {
	margin: 0;
	outline: none;
	padding: 0;
}

.clearfix:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

.fl {
	float: left;
}

.fr {
	float: right;
}

#header {
	height: 40px;
	background-color: #66c2ff;
	width: 100%;
	line-height: 40px;
}

.header_top {
	width: 1200px;
	margin: 0 auto;
}

.header_top i {
	display: inline-block;
	height: 25px;
	width: 25px;
	vertical-align: middle;
}

.header_top .header_top_lf li {
	float: left;
	color: #fff;
	border-right: 1px solid #fff;
	padding: 0 10px;
	height: 40px;
	line-height: 40px;
	font-size: 16px;
	cursor: pointer;
	position: relative;
}

.header_top_lf span {
	vertical-align: middle;
	color: #fff;
}

#header .header_top .icon_home {
	background: url("../images/get_back.png") no-repeat left center;
}

.header_top .portrait {
	background: url("../images/portrait.png") no-repeat left center;
	border-radius: 20px;
	overflow: hidden;
}

#header .header_top #money {
	color: #ff6633;
}

.header_top .portrait+span {
	padding-left: 5px;
}

#header #money span {
	color: #ff6633;
	vertical-align: middle;
	font-size: 16px;
	display: inline-block;
	margin-top: -4px;
}

.header_menu_wrap {
	width: 100%;
	background-color: #fff;
}

.header_menu {
	height: 80px;
	width: 1200px;
	margin: 0 auto;
	text-align: center;
}

.banner {
	height: 480px !important;
	background: #565756;

}
.banner img{
	height: 480px;
}
.header_top_rt {
	float: right;
}

.header_top .header_top_rt li {
	float: left;
	cursor: pointer;
	position: relative;
	height: 40px;
}

.header_top_rt span {
	color: #fff;
}

.header_top_rt .pay_icon+span {
	color: #ffd76c;
}

.header_top_rt .logout_icon+span {
	color: #ff5c5c;
}

.header_top_rt li {
	padding: 0 10px;
	border-left: 1px solid #fff;
}

.header_top li:first-child {
	border-right: 1px solid #fff;
}

.header_top .pay_icon {
	background: url("../images/pay_icon.png") no-repeat left center;
}

.header_top .draw_icon {
	background: url("../images/draw_icon.png") no-repeat left center;
}

.header_top .report_icon {
	background: url("../images/report_icon.png") no-repeat left center;
}

.header_top .group_icon {
	background: url("../images/group_icon.png") no-repeat left center;
}

.header_top .account_icon {
	background: url("../images/account_icon.png") no-repeat left center;
}

.header_top .logout_icon {
	background: url("../images/logout_icon.png") no-repeat left center;
}

.header_menu img {
	float: left;
	margin-top: 10px;
}

.header_menu ul {
	float: right;
}

.header_menu ul li {
	float: left;
	height: 80px;
	line-height: 80px;
	padding: 0 21px;
}

.header_menu ul li a {
	padding: 10px 9px;
	color: #666;
}

.header_menu ul li a:hover {
	color: #333;
	cursor: pointer;
}

.header_menu ul li a.active {
	border-bottom: 3px solid #66c2ff;
}


/*banner*/

.banner {
	width: 100%;
	height: 420px;
	overflow: hidden;
	position: relative;
}

.banner .img {
	width: 15000px;
	position: absolute;
	left: 0px;
	top: 0;
}

.banner .img li {
	float: left;
}

.banner .num {
	position: absolute;
	width: 100%;
	bottom: 10px;
	left: 0;
	text-align: center;
	font-size: 0px;
}

.banner .num li {
	width: 10px;
	height: 10px;
	background: #888;
	border-radius: 50%;
	display: inline-block;
	margin: 0 3px;
	cursor: pointer;
}

.banner .num li.on {
	background: #F60;
}

.banner .btn {
	width: 30px;
	height: 50px;
	background: rgba(0, 0, 0, 0.5);
	position: absolute;
	top: 50%;
	margin-top: -25px;
	cursor: pointer;
	text-align: center;
	line-height: 50px;
	color: #fff;
	font-size: 40px;
	font-family: "宋体";
	display: none;
}

.banner:hover .btn {
	display: block;
}

.banner .btn_l {
	left: 0;
}

.banner .btn_r {
	right: 0;
}


/*section*/

#section {
	padding: 0 0 50px 0;
}

#section .notice_wrap {
	background: #66c2ff;
}

#section .notice {
	height: 40px;
	width: 1200px;
	margin: 0 auto;
	clear: both;
	line-height: 40px;
}

#section .notice strong {
	width: 27px;
	height: 24px;
	margin-top: 8px;
}

#section .notice strong:first-child {
	background: url("../images/notice.png");
}

#section .notice strong:last-child {
	background: url("../images/record_icon.png");
}


/*模态框*/


/*遮罩层*/

#m_modal {
	width: 100%;
	height: 100%;
	display: none;
}

#m_modal .modal_shutter {
	width: 100%;
	height: 2000px;
	position: fixed;
	top: 0;
	left: 0;
	background-color: #000000;
	opacity: 0.5;
	z-index: 9999;
}

#m_modal .popups {
	position: fixed;
	top: 24%;
	left: 24%;
	width: 1000px;
	height: 600px;
	z-index: 10000;
	background-color: white;
	border-radius: 23px;
}

#m_modal .popups .popups_header {
	width: 100%;
	height: 50px;
	background-color: #66C2FF;
	border-top-left-radius: 23px;
	border-top-right-radius: 23px;
	position: relative;
}

#m_modal .popups .popups_inform {
	display: inline-block;
	padding-top: 10px;
	margin-left: 20px;
	margin-right: 10px;
	float: left;
}

#m_modal .popups .popups_inform_title {
	font-size: 18px;
	width: 100px;
	line-height: 50px;
	float: left;
	color: #FFFFFF;
}

#m_modal .popups .popups_close {
	width: 40px;
	height: 40px;
	position: absolute;
	top: -10px;
	right: -20px;
}

#m_modal .popups .popups_close:hover {
	cursor: pointer;
}

#m_modal .popups .popups_close img {
	width: 100%;
}


/*主体内容左侧*/
/*.scroll_hide{
	width: 230px;
	height: 550px;
	float: left;
}*/
#m_modal .popups .popups_body .popups_body_left {
	height: 550px;
	width: 230px;
	padding-right: 10px;
	overflow: hidden;
	float: left;
}
.Vertical {
	border-right: 3px solid #579BC8;
	width: 230px;
	height: 550px;
	position: absolute;
}
#m_modal .inform_list {
	float: left;
	width: calc(100% + 27px);
	height: calc(100% + 27px);
	overflow: scroll;
}

#m_modal .inform_item {
	width: 200px;
	margin: 10px 10px 20px 10px;
	text-align: center;
	border: 1px solid #DFDFDF;
	border-radius: 10px;
	height: 70px;
	position: relative;
}

#m_modal .inform_item:hover {
	cursor: pointer;
}

#m_modal .inform_item p:nth-child(1) {
	color: #333333;
	font-size: 16px;
	width: 100px;
	margin: auto;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	margin-bottom: 10px;
	line-height: 20px;
	padding-top: 10px;
}

#m_modal .inform_item p:nth-child(2) {
	color: #999999;
	font-size: 14px;
	line-height: -10px;
}

#m_modal .inform_item .globule {
	display: block;
	width: 15px;
	height: 15px;
	border-radius: 50%;
	background-color: #579BC8;
	position: absolute;
	right: -28px;
	top: 40%
}

#m_modal .inform_active {
	background-color: #66C2FF;
}

#m_modal .inform_active p:nth-child(1) {
	color: #FFFFFF;
}

#m_modal .inform_active p:nth-child(2) {
	color: #c8e9ff;
}


/*主体内容右侧*/

#m_modal .popups .popups_body .popups_body_right {
	width: 740px;
	height: 500px;
	float: left;
}

#m_modal .popups .popups_body .popups_body_right_container {
	display: none;
}

#m_modal .popups .popups_body_right h2 {
	font-size: 18px;
	color: #333333;
	margin: 30px 0 40px;
	text-align: center;
}

#m_modal .popups .popups_body_right p {
	margin: 40px 50px 50px;
	font-size: 14px;
	text-indent: 28px;
}
.notice_wrap .notice_content {
	height: 40px;
	line-height: 40px;
	float: left;
	width: 1070px;
	margin: 0 auto;
	padding-left: 20px;
	overflow: hidden;
	position: relative;
	left: 0;
}

#notice_bar {
	position: absolute;
	color: #fff;
	width: auto;
	left: 200px;
	height: 40px;
	line-height: 40px;
}

.section_content {
	width: 1080px;
	margin: 0 auto;
}

.section_content>div {
	height: 200px;
	width: 350px;
	position: relative;
	left: 0;
	float: left;
	margin: 15px 15px 0 0;
	overflow: hidden;
}

#section .section_content>div:nth-child(3),
#section .section_content>div:nth-child(6) {
	margin-right: 0;
}

.section_content>div div {
	cursor: pointer;
	position: absolute;
	top: 0;
	left: 0;
}

.section_content>div div img {
	position: absolute;
	top: 60px;
	left: 20px;
}

.section_content>div div h1 {
	position: absolute;
	left: 110px;
	top: 86px;
	width: 86px;
	padding-right: 10px;
	border-right: 3px solid #fff;
	color: #fff;
	font-size: 28px;
}

.section_content>div div ul {
	position: absolute;
	left: 218px;
	top: 43px;
	width: 80px;
}

.section_content>div ul li {
	margin-bottom: 5px;
}

.section_content>div ul a {
	color: #fff;
	font-size: 16px;
}

.section_content>div ul a:hover {
	color: #5794ca;
}

.section_content>div .bg_animate {
	transition: all 0.6s;
}

.section_content>div:hover .bg_animate {
	cursor: pointer;
	transform: scale(1.4);
}

#section .variety_min ul {
	top: 33px;
}

#section .variety_sharp img,
#section .variety_else img {
	left: 50px;
}

#section .variety_sharp h1,
#section .variety_else h1 {
	left: 140px;
	padding-right: 10px;
	width: 58px;
}

#section .variety_sharp ul {
	top: 44px;
}

#section .variety_sec ul {
	width: 120px;
}

#section .variety_ten img {
	left: 13px;
}

#section .variety_ten h1 {
	left: 102px;
	width: 118px;
}

#section .variety_ten ul {
	left: 242px;
	width: 108px;
	top: 66px;
}

#section .variety_else ul {
	top: 31px;
}


/*footer*/

.footer_top_wrap {
	height: 80px;
	background: #66c2ff;
	width: 100%;
}

.footer_top {
	height: 80px;
	width: 1200px;
	margin: 0 auto;
}

.footer_top strong {
	color: #fff;
	height: 80px;
	line-height: 80px;
}

.footer_msg ul {
	margin: 0 auto;
	padding: 30px 0 10px 0;
}

.footer_msg {
	text-align: center;
}

.footer_msg ul li {
	display: inline-block;
	padding-right: 30px;
	padding-bottom: 10px;
	color: #333;
	font-size: 14px;
	cursor: pointer;
}

.footer_msg ul li:last-child {
	padding-right: 0;
}

.footer_msg p {
	font-size: 14px;
	clear: both;
	color: #333;
}

.copyright {
	padding-bottom: 10px;
}


/*报表查询开始*/

#team_ad,
#statement,
#recharge,
#acc_record,
#user_manage {
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	background: rgba(0, 0, 0, .7);
	z-index: 999;
	display: none;
}

#team_ad .statement_tab,
#statement .statement_tab,
#recharge .statement_tab,
#acc_record .statement_tab,
#user_manage .statement_tab {
	position: absolute;
	left: 50%;
	top: 50%;
	width: 1000px;
	min-height: 580px;
	background-color: #fff;
	margin-left: -500px;
	margin-top: -290px;
	z-index: 10010;
	border-radius: 20px;
}

.tab_list {
	width: 1000px;
	height: 50px;
	font-size: 18px;
	position: relative;
	border-radius: 20px 20px 0 0;
	background-color: #66c2ff;
}

.tab_list ul {
	float: left;
}

.tab_list li {
	float: left;
	height: 50px;
	width: 90px;
	line-height: 50px;
	text-align: center;
	cursor: pointer;
	color: #fff;
}

.tab_list li:hover {
	background-color: #2D83CA;
}

.tab_list li.active {
	background-color: #579bc8;
}

.tab_list .state_close {
	cursor: pointer;
	position: absolute;
	height: 50px;
	width: 50px;
	right: -25px;
	top: -25px;
	border-radius: 50%;
	overflow: hidden;
	background: url("../images/off.png") no-repeat;
}

.tab_content {
	position: relative;
}

.tab_content .account_content {
	min-height: 500px;
	padding: 15px;
	display: none;
}

.tab_content .account_content.active {
	display: block;
	position: relative;
}

.tab_list i {
	margin: 9px 32px 0 34px;
	float: left;
	height: 30px;
	width: 30px;
	background: url("../images/statement_icon.png") no-repeat;
}

.search_date {
	background-color: #f5f5f5;
	border-radius: 10px;
	min-height: 66px;
	width: 940px;
	padding: 7px 0 7px 15px;
	margin-bottom: 15px;
	color: #999999;
}

.shortcut_time {
	height: 30px;
}

.shortcut_time li {
	float: left;
	height: 26px;
	width: 58px;
	color: #999999;
	font-size: 14px;
	line-height: 26px;
	text-align: center;
	border: 1px solid #999999;
	margin-right: 10px;
	border-radius: 2px;
}

.shortcut_time li.item:hover {
	color: #66c2ff;
	border-color: #66c2ff;
	cursor: pointer;
}

.shortcut_time li.item.on {
	color: #3cafee;
	border-color: #3cafee;
}

.tab_content .shortcut_time li:first-child {
	margin-right: 5px;
	border: none;
	width: 66px;
}

.search_date label {
	display: inline-block;
	font-size: 14px;
	color: #999999;
	margin-top: 6px;
	padding-left: 3px;
}

.search_date label input {
	margin-left: 3px;
	width: 135px;
	border: 1px solid #999999;
	border-radius: 2px;
	height: 26px;
	cursor: pointer;
}

.search_date .btn_search {
	margin-left: 10px;
	margin-top: 1px;
	display: inline-block;
	height: 28px;
	width: 94px;
	line-height: 28px;
	color: #fff;
	border-radius: 5px;
	background-color: #66c2ff;
	vertical-align: middle;
	cursor: pointer;
}

.search_date .btn_search i {
	display: inline-block;
	height: 12px;
	width: 13px;
	background: url("../images/search.png") no-repeat;
	margin-left: 23px;
}

.state_tab {
	border-collapse: collapse;
	color: #666;
	text-align: center;
	font-size: 12px;
	width: 100%;
}

.state_tab th {
	height: 20px;
	line-height: 20px;
	padding: 3px;
	text-align: center;
	border: 1px solid #a3d3e9;
	background-color: #c8e7fc;
	white-space: nowrap;
}

.state_tab td {
	height: 20px;
	line-height: 20px;
	padding: 3px;
	text-align: center;
	border: 1px solid #a3d3e9;
	white-space: nowrap;
}

.state_tab td a {
	color: #2d97dd;
}

.state_tab tr {
	display: table-row;
	vertical-align: inherit;
	border-color: inherit;
}

.state_tab tr:hover {
	cursor: pointer;
	background-color: #f5f5f5;
}

.state_tab_no tr:hover {
	cursor: default;
}

.state_tab_no tr {
	cursor: default;
}

.panging {
	width: 96%;
	padding: 5px 2%;
	font-size: 14px;
}

.page_jump {
	height: 20px;
}

.page_jump span {
	height: 20px;
	line-height: 20px;
	display: inline-block;
	text-align: center;
	margin: 0 2px;
	cursor: default;
	font-size: 14px;
}

.page_jump span.active {
	cursor: pointer;
	width: auto;
	color: #2d97dd
}

.page_jump input {
	width: 30px;
	padding: 0;
	height: 18px;
	line-height: 18px;
	border: 1px solid #ececec;
}

.page_jump .go {
	background: #31adff;
	color: #fff;
	width: 30px;
	cursor: pointer;
	border-radius: 2px;
}

.search_date .shortcut_btn {
	height: 36px;
	color: #999;
	line-height: 36px;
	font-size: 14px;
	display: inline-block;
	vertical-align: middle;
	margin-top: 2px;
}

.search_date .shortcut_btn li {
	height: 26px;
	line-height: 26px;
	cursor: pointer;
	width: 60px;
	margin: 3px 5px;
	border: 1px solid #C6C6C6;
	float: left;
	border-radius: 5px;
	text-align: center;
}

.shortcut_btn li.on {
	color: #3cafee;
	border-color: #3cafee;
}

.sort,
.use_name {
	border: 1px solid #999999;
	margin-left: 3px;
	border-radius: 2px;
	height: 26px;
	width: 100px;
}


/*报表查询结束*/


/*充值提现*/

#recharge .chongzhi {
	background: url('../images/chongzhi.png') no-repeat;
}

.account_content .velocity {
	padding: 85px 0 0 55px;
	color: #999;
}

.velocity .sel_bank {
	margin-bottom: 45px;
}

.velocity .sel_bank input,
.bank_card .sel_bank input {
	width: 20px;
	height: 28px;
	border-color: 66c2ff;
	margin: 0 5px 0 10px;
}

.sel_bank img {
	width: 200px;
	height: 50px;
}

.bank_card .sel_bank img {
	width: 180px;
	height: 40px;
}

#credit,
#weichar_cre {
	width: 198px;
	height: 28px;
	border: 1px solid #999;
	border-radius: 5px;
	margin-left: 10px;
}

i.zhuyi {
	display: inline-block;
	height: 15px;
	width: 15px;
	background: url("../images/zhuyi.png") no-repeat;
	margin: 0 10px;
	vertical-align: middle;
}

.velocity .quota,
.deposit_wrap .quota {
	padding-left: 89px;
	margin: 25px 0 20px;
}

#recharge .velocity strong,
.deposit_wrap {
	color: 66c2ff;
}

.quota strong {
	color: #66c2ff;
}

.submit_deposit {
	cursor: pointer;
	display: inline-block;
	color: #fff;
	background: #66c2ff;
	height: 50px;
	line-height: 50px;
	width: 180px;
	border-radius: 5px;
	text-align: center;
	margin-left: 89px;
}

.deposit_wrap {
	margin-left: 260px;
	padding-top: 15px;
	color: #666;
}

.deposit_wrap img {
	display: block;
	margin-left: 75px;
	margin-bottom: 60px;
}

.attention_list {
	margin-top: 25px;
	color: #666;
	font-size: 14px;
}

.attention_list strong {
	display: block;
	margin-bottom: 5px;
}

.attention_list p {
	padding-top: 2px;
}

.bank_card {
	margin-left: 20px;
	color: #666;
}

.bank_card .sel_bank {
	margin: 35px 0 60px 35px;
}

.bank_dep input {
	width: 198px;
	height: 28px;
	border: 1px solid #666;
	border-radius: 5px;
}

.bank_dep div {
	margin: 30px 0 0 13px;
}

.bank_dep .quota {
	padding-left: 80px;
	margin: 25px 0 20px;
