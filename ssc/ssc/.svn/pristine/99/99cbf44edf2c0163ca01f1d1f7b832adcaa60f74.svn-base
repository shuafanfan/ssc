/* 线路检测弹窗 */
.xljc_tanc { position: fixed; z-index: 1001; top: 50%; left: 50%; margin: -340px 0 0 -500px; width: 1000px; height: 680px; font-family: "微软雅黑" ,"宋体" , Arial; display: none; }
.xljc_tanc_top { position: relative; height: 50px; background: rgb(102,194,255); border-radius: 20px 20px 0 0;  }
.xljc_tanc_top  p { line-height: 50px; font-size: 18px; color: #fff;  }
.xljc_tanc_top  p img { float: left; margin: 7.5px 10px 0 20px; }
.xljc_tanc_top a { display: block; position: absolute; z-index: 2; right: -24px; top: -24px;  cursor: pointer; }
.xljc_tanc_top a img { display: block;  width: 100%; height: 100%; }
.xljc_tanc_title { padding-top: 30px; height: 60px; margin: 0 auto ; font-size: 30px; color: #333; text-align: center;  }
.xljc_tanc_title  img { vertical-align: middle; }
.xljc_tanc_body {
	padding: 0 130px; border-radius: 0px 0px 20px 20px; background: #fff; height: 630px; overflow: hidden;
}

.xljc_tanc_body p {
	margin: 30px auto 0;
	width: 640px;
	height: 35px;
	line-height: 35px;
	text-align: left;
	font-size: 16px;
	color: #666;
}
.xljc_tanc_body p a {
	text-decoration: none;
	background: #81a7e7;
	width: 60px;
	border-radius: 5px;
	display: inline-block;
	color: #fff;
	text-align: center;
	float: right;
}
.xljc_tanc_body p a:hover { background: #9bf; }
.xljc_tanc_body p a:active { background: #79d; }
.xljc_tanc_body dl {
	height: 276px; overflow-y: auto; margin: 0 auto; 	padding: 20px 0 0; width: 640px;
}
.xljc_tanc_body dl::-webkit-scrollbar {width:6px;height:6px}
.xljc_tanc_body dl::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0); 
    -webkit-border-radius: 10px;
    }

.xljc_tanc_body dl::-webkit-scrollbar-thumb {
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background:rgba(255, 255, 255, 0.5);
    -webkit-box-shadow: inset 0 0 6px rgba(216, 208, 208, 0.1);; 
              
}
.xljc_tanc_body dl::-webkit-scrollbar-thumb:window-inactive {
background: rgba(81, 75, 75,0.4); 
              
}
.xljc_tanc_body dd {
	height: 35px;
	line-height: 35px;
	padding: 10px 0;
	font-size: 16px;
}
.xljc_tanc_body dd div {
	background: #579bc8;
	width: 360px;
	height: 35px;
	border-radius: 17px;
	line-height: 35px;
	color: #fff;
	text-align: center;
	float: right;
}
.xljc_tanc_body dd a {
	text-decoration: none;
	background: #66c2ff;
	width: 60px;
	height: 35px;
	line-height: 35px;
	border-radius: 5px;
	margin: 0 0 0 50px;
	color: #fff;
	text-align: center;
	float: right;
}
.xljc_tanc_body dd a:hover { background: #7ef; }
.xljc_tanc_body dd a:active { background: #5de; }
.xljc_tanc_body dd span {
	background: #57c89c;
	width: 60px;
	height: 35px;
	padding: 0;
	line-height: 35px;
	display: inline-block;
	border-radius: 5px;
	color: #fff;
	text-align: center;
}
