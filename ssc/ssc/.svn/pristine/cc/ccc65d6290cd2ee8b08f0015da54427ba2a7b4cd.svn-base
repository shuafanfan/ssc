var password=document.getElementById("password");
var npassword=document.getElementById("npassword");
var npasswordTip=document.getElementById("npasswordTip");
npassword.onblur=function(){
if(password.value != npassword.value) {
    npasswordTip.innerHTML = "您输入的密码不一致,请重新输入";
    npasswordTip.style.color = "#e4393c";
}
};
npassword.onfocus=function(){
    npasswordTip.innerHTML="";
};
password.onfocus=function(){
    npasswordTip.innerHTML="";
};
