html{overflow:hidden;}
html,body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,em,form,fieldset,input,textarea,p,span,input,blockquote,th,td,a{margin:0;padding:0;font-family: "Microsoft YaHei", Arial;font-style:normal}
body{font-family: "Microsoft YaHei", Arial;font-size:16px;color:#000000;word-wrap:break-word;overflow:hidden;}
button,input,select,textarea{font:16px/1.5  Arial, Helvetica,  Verdana, sans-serif;word-wrap:break-word;color:#565756;}
ul,li{list-style:none;margin:0;padding:0;}
h1, h2, h3, h4, h5, h6{font-weight:normal;font-size:100%;}
img{border:none;border:0;vertical-align:middle;}
a{text-decoration:none;outline:none;}
p{margin:0;padding:0;}
input,select,textarea{vertical-align:middle;*font-size:100%;}
input{margin:0;outline:none;padding:0;}
.clearfix:after{
    content:"";
    display:block;
    height:0;
    clear:both;
    visibility:hidden;

}
.clearfix{*zoom:1}
.ddw{position:absolute;left:0;top:0;color:#000;font-size:16px;z-index:9999;}
.ddw2{position:absolute;left:20px;top:0;color:#000;font-size:16px;z-index:9999;}
.num_box{position:relative;}
.num{position:relative;}
#num_0{color:#90C;font-size:50px;width:100%;text-align:center;position:relative;}
#num_0 p{position:absolute;top:0;left:50%;margin-left:-66px;}
#num_0 ul{width:100%;position:absolute;left:0;top:0;height:100%;}
#num_0 ul li{float:left;height:100%;position:relative;}
#num_0{background:url(../images/bg1.png) no-repeat center #03F;background-size:100% 100%;}
#num_1{background:url(../images/bg2.png) no-repeat center #03F;background-size:100% 100%;}
#num_2{background:url(../images/bg3.png) no-repeat center #03F;background-size:100% 100%;}
#num_3{background:url(../images/bg4.png) no-repeat center #03F;background-size:100% 100%;}
#num_4{background:url(../images/bg5.png) no-repeat center #03F;background-size:100% 100%;}
#num_5{background:url(../images/bg6.png) no-repeat center #03F;background-size:100% 100%;}
/*111*/
.fixed_list{
    width:100%;
    height:80px;
    position:fixed;
    top:0;
    left:0;
    z-index: 999;
    line-height:80px;
}
.fixed_list ul{
    width:684px;
    margin:0 auto;
    background-color:blue;
}
.fixed_list ul li{
    float:left;
    height:80px;
    line-height:80px;
    width:114px;
    text-align: center;
    color:#fff;
    cursor:pointer;
    font-size:16px;
}
.fixed_list li.on{
    border-bottom:3px solid #4EC6FD;
}

#login{
    width:500px;
    position:absolute;
    left:50%;
    top:50%;
    margin-left:-250px;
    margin-top:-250px;

}
#login .logo{
    float:left;
    position:relative;
    top:0;
    left: 0;
}
#login .logo h1{
    font-size:48px;
    float:right;
    color:#fff;
    margin-left:50px;
}
#login .logo span{
    color:#fff;
    position:absolute;
    left:220px;
    top:80px;
    font-size:24px;
}
#login .logo .system{
    background:url("../images/Betting-system.png") no-repeat left;
    height:102px;
    width:230px;
    float:right;
    margin-left:50px;
    margin-top:18px;
}
#login .login_main{
    float:left;
    margin-top:60px;
}
#login .login_main input,span{
    font-family: "Microsoft YaHei", Arial;
    height:50px;
    line-height:50px;
    padding-left:20px;
    border-radius: 5px;
    border-style: none;
}
#username,#password{
    width:480px;
}
#username{
    margin-bottom: 40px;
}

#login .login_main .code_message{
    width:160px;
    height:50px;
    background-color:#fff;
	border-radius: 5px;
    margin-top: 30px;
    float: right;
}
#code{
    width:280px;
    float: left;
    margin-top: 50px;
    vertical-align: middle;
}
#login #login_btn{
    width:500px;
    height:60px;
    background-color:#66ccff;
    line-height:60px;
    font-size:24px;
    color:#fff;
    margin-top:40px;
    padding:0;
    font-family: "Microsoft YaHei", Arial;
    cursor:pointer;
}
#login .login_main .forget{
    float:right;
    font-size:14px;
    color:#fff;
}
/*login_cs*/
.login_cs {
	background: #fff;
	width: 640px;
	height: 500px;
	padding: 100px 130px 0;
	border-radius: 10px;
	position:absolute;
	top:180px;
	left: 50%;
	margin: 0px 0 0 -450px;
}

.login_cs p {
	height: 35px;
	line-height: 35px;
	text-align: left;
	font-size: 16px;
	color: #666;
	position: static;
    top: 0;
    left: 0;
    margin-left: 0;
}
.login_cs p a {
	background: #81a7e7;
	width: 60px;
	border-radius: 5px;
	display: inline-block;
	color: #fff;
	text-align: center;
	float: right;
}
.login_cs dl {
	padding: 50px 0 0;
}
.login_cs dd {
	height: 35px;
	line-height: 35px;
	padding: 10px 0;
}
.login_cs dd div {
	background: #579bc8;
	width: 360px;
	height: 35px;
	border-radius: 17px;
	line-height: 35px;
	color: #fff;
	text-align: center;
	float: right;
}
.login_cs dd a {
	background: #66c2ff;
	width: 60px;
	height: 35px;
	line-height: 35px;
	border-radius: 5px;
	margin: 0 0 0 50px;
	color: #fff;
	text-align: center;
	float: right;
}
.login_cs dd span {
	background: #57c89c;
	width: 60px;
	height: 35px;
	padding: 0;
	line-height: 35px;
	display: inline-block;
	border-radius: 5px;
	color: #fff;
	text-align: center;
}
.login_cs dd span.span_0{
	background: #57c89c;
	width: 60px;
	height: 35px;
	padding: 0;
	line-height: 35px;
	display: inline-block;
	border-radius: 5px;
	color: #fff;
	text-align: center;
}
.login_cs dd span.span_1 {
	background: #66c2ff;
}
.login_cs dd span.span_2 {
	background: #f9bb4f;
}
.login_cs dd span.span_3 {
	background: #f66556;
}
.login_cs dd i {
	margin: 0 0 0 10px;
	font-style: normal;
}
/*login_cs end*/
