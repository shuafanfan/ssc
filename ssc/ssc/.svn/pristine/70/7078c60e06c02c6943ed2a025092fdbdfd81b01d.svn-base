.wheel {
	width: 160px;
	height: 160px;
	border-radius: 50%;
	margin-left: 17px;
	position: absolute;
	top: 10px;
}

.wheel .indicator {
	width: 100%;
	height: 100%;
	position: absolute;
}

.wheel .indicator:after {
	content: '';
	display: block;
	position: absolute;
	background: #fff;
	border-radius: 50%;
	width: 20px;
	height: 20px;
	top: 102px;
	left: 9px;
	margin: auto;
	box-shadow: 0px 0px 2px 2px rgba(0, 0, 0, 0.2);
}

.wheel_number {
	width: 156px;
	position: absolute;
	top: 124px;
	left: 0;
	font-size: 12px;
}

.wheel_number .fl {
	width: 66px;
	text-align: center;
	float: left;
}

.wheel_number .fr {
	width: 50px;
	padding-right: 12px;
	float: right;
}

.wheel_number .fr div:nth-child(1) {
	padding-left: 12px;
}

.wheel_number_h {
	background: #579bc8;
	width: 210px;
	height: 32px;
	line-height: 32px;
	font-size: 14px;
	border-radius: 5px;
	position: absolute;
	bottom: 40px;
	right: -74px;
	color: #fff;
	text-align: center;
	display: none;
}

.wheel_number_h span {
	width: 92px;
	display: inline-block;
}

.wheel_number_h span span {
	width: auto;
	display: inline;
}

.wheel_number_h:after {
	content: "";
	width: 0;
	height: 0;
	border-left: 6px solid transparent;
	border-right: 6px solid transparent;
	border-top: 10px solid #579bc8;
}

.wheel_number .fr:hover .wheel_number_h {
	display: block;
}

.outline {
	width: 12.5em;
	height: 12.5em;
	-webkit-transform: translateZ(0) rotateZ(90deg);
	transform: translateZ(0) rotateZ(90deg);
}

@-webkit-keyframes rotate {
	40% {
		-webkit-transform: rotateZ(90deg);
		transform: rotateZ(90deg);
	}
	100% {
		-webkit-transform: rotateZ(0);
		transform: rotateZ(0);
	}
}

@keyframes rotate {
	40% {
		-webkit-transform: rotateZ(90deg);
		transform: rotateZ(90deg);
	}
	100% {
		-webkit-transform: rotateZ(0);
		transform: rotateZ(0);
	}
}

