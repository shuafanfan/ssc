html,
body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
em,
form,
fieldset,
input,
textarea,
p,
span,
strong,
input,
i,
blockquote,
th,
td,
a {
	margin: 0;
	padding: 0;
	font-family: "Microsoft YaHei", Arial;
	font-style: normal;
}

body {
	font-family: "Microsoft YaHei", Arial;
	font-size: 16px;
	color: #000000;
}

button,
input,
select,
textarea {
	font: 16px/1.5 Arial, Helvetica, Verdana, sans-serif;
	word-wrap: break-word;
	color: #565756;
}

ul,
li {
	list-style: none;
	margin: 0;
	padding: 0;
}

h1,
h2,
h3,
h4,
h5,
h6 {
	font-weight: normal;
	font-size: 100%;
}

img {
	border: none;
	border: 0;
	vertical-align: middle;
}

a {
	text-decoration: none;
	outline: none;
}

p {
	margin: 0;
	padding: 0;
}

input,
select,
textarea {
	vertical-align: middle;
	*font-size: 100%;
}

input {
	margin: 0;
	outline: none;
	padding: 0;
}

.clearfix {
	clear: both;
}

.clearfix:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

.fl {
	float: left;
}

.fr {
	float: right;
}


